      \strng{namehash}{92cf8146b8cb20192acab688d3a32288}
      \strng{fullhash}{ccb767bda5b1858c745b8b996c2d791e}
      \strng{bibnamehash}{ccb767bda5b1858c745b8b996c2d791e}
